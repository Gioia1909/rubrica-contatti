<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="336.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="it.university.group9.rubricacontattigroup9.SelezionaContattiDaRubricaController">
    <stylesheets>
        <URL value="@/styles/selezionacontattidarubrica.css" />
    </stylesheets>
    <children>
        <VBox prefHeight="400" prefWidth="336.0" spacing="10">
           <children>
               <TextField fx:id="searchBar" prefHeight="27.0" prefWidth="267.0" promptText="Cerca un contatto..." />
               <ListView fx:id="contactListView" prefHeight="300" prefWidth="325.0" />
               <HBox alignment="CENTER" spacing="10">
                  <children>
                      <Button fx:id="addButton" onAction="#handleAddContact" prefHeight="29.0" prefWidth="17.0">
                     <graphic>
                        <ImageView fitHeight="23.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@add.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                      <Button fx:id="closeButton" onAction="#handleClosePopup">
                     <graphic>
                        <ImageView fitHeight="28.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@back.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  </children>
               </HBox>
           </children>
        </VBox>
    </children>
</AnchorPane>
